
<%- include("partials/header"); -%>
    <h1>Compose</h1>
    <form class="" action="/compose" method="post">
      <div class="form-group">
        <label>Title</label>
        <input class="form-control" type="text" name="postTitle">
        <label>Post</label>
        <textarea class="form-control" name="postBody" rows="5" cols="30" style="font-size:125%"></textarea>
      </div>
      <button class="btn btn-primary" type="submit" name="button">Publish</button>
      <br><br>
      <a class="btn btn-primary" href="/logout" role="button">Log Out</a>
    </form>

<%- include("partials/footer"); -%>
